{
  "packages": [
    "packages/razzle",
    "packages/babel-preset-iabbb",
    "packages/razzle-dev-utils",
    "packages/i18n-loader",
    "packages/svg-icon-loader",
    "packages/eslint-config-iabbb",
    "packages/prettier-config-iabbb",
    "packages/utils",
    "examples/*"
  ],
  "useWorkspaces": true,
  "npmClient": "yarn",
  "command": {
    "bootstrap": {
      "hoist": true,
      "nohoist": "express"
    }
  },
  "changelog": {
    "repo": "IABBB/razzle",
    "labels": {
